{
  "name": "info-assistant",
  "version": "1.0.0",
  "description": "Daily information provider",
  "main": "index.js",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.14.0",
    "yarn": "4.0.2"
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "get:info:openings-cr": "node --env-file=.env src/openings-cr",
    "get:info:openings-mj": "node --env-file=.env src/openings-mj",
    "get:info:openings-yr": "node --env-file=.env src/openings-yr",
    "get:info:travel-taipei": "node --env-file=.env src/travel-taipei",
    "get:info:weather": "node --env-file=.env src/weather"
  },
  "dependencies": {
    "cheerio": "1.0.0-rc.12",
    "nodemailer": "^6.9.13"
  },
  "author": "chejen",
  "license": "MIT"
}
